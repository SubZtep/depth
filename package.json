    "@vueuse/core": "^6.7.3",
    "vue": "^3.2.20"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@types/node": "^16.11.4",
    "@vue/test-utils": "^2.0.0-rc.16",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "jest": "^27.3.1",
