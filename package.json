{
  "name": "depth",
  "version": "0.1.0",
  "private": true,
  "description": "depth test",
  "scripts": {
    "build": "pnpm recursive run build",
    "wipe": "pnpm recursive exec -- rm -rf node_modules dist tsconfig.tsbuildinfo",
    "dev": "pnpm recursive run dev",
    "web": "pnpm run dev watch --filter @depth/web",
    "serve": "pnpm recursive run serve --filter @depth/web",
    "lint": "pnpm recursive exec eslint .",
    "test": "pnpm test --filter @depth/web",
    "preinstall": "npx only-allow pnpm"
  },
  "author": "Andras Serfozo <subztep@gmail.com>",
  "license": "Unlicense",
  "repository": "github:SubZtep/depth",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0",
    "typescript": "^4.4.4"
  },
  "pnpm": {
    "neverBuiltDependencies": [
      "node-gyp"
    ]
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  }
}
