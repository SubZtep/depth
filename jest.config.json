{
  "verbose": true,
  "preset": "ts-jest",
  "moduleFileExtensions": ["vue", "ts", "js"],
  "transform": {
    "^.+\\.vue$": "vue3-jest"
  },
  "testEnvironment": "jsdom",
  "coveragePathIgnorePatterns": ["/node_modules/"],
  "globals": {
    "ts-jest": {
      "tsconfig": "tsconfig.json"
    },
    "vue-jest": {
      "babelConfig": false
    }
  }
}
