{
  "deno.enable": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "css.validate": false,
  "css.lint.emptyRules": "ignore",
  "files.associations": {
    ".templates/**/*.ts": "plaintext",
    ".prettierrc": "yaml",
    "**/.eslintrc": "yaml"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.exclude": {
    "**/.git": true,
    "**/*.js": true,
    "**/*.d.ts": true,
    "**/*.spec.ts": true,
    "**/build": true,
    "**/node_modules": true
  },
  "files.exclude": {
    "**/dist": false,
    "**/node_modules": false,
    "**/tsconfig.tsbuildinfo": true,
    "pnpm-lock.yaml": true,
    "**/.wireit": true
  },
  "files.watcherExclude": {
    "**/node_modules": true,
    "**/build": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[postcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "lit-plugin.rules.no-unknown-attribute": "off",
  "lit-plugin.rules.no-invalid-css": "off",
  "window.title": "d̸͔́è̶̖̝̭m̸̯̪͆̋̈́̉͜o̵͖͍̪̮̾́̀̎.̶̧̰̫̤͐̔̉́̎l̸̡̛͕͍̞a̷̮̾͘ń̵̠̯d̶͈̫̎͝ⒹⒺⓅⓉⒽ 龴ↀ◡ↀ龴 ▂▃▅▇█▓▒░ ${activeEditorMedium}  ░▒▓█▇▅▃▂ ⺌${dirty}‿${dirty}⺌ ⒹⒺⓅⓉⒽd̸͔́è̶̖̝̭m̸̯̪͆̋̈́̉͜o̵͖͍̪̮̾́̀̎.̶̧̰̫̤͐̔̉́̎l̸̡̛͕͍̞a̷̮̾͘ń̵̠̯d̶͈̫̎͝",
  "workbench.colorTheme": "Red",
  "html.customData": [
    ".vscode/wc.json"
  ],
  "folderTemplates.templateFolderPath": ".templates"
}
