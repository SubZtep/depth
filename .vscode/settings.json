{
  "deno.enable": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "windicss.enableCodeFolding": true,
  "windicss.foldByLength": true,
  "css.validate": false,
  "css.lint.emptyRules": "ignore",
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "files.associations": {
    ".*rc": "yaml"
  },
  "jest.autoRun": "off",
  "editor.quickSuggestions": {
    "strings": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.exclude": {
    "**/.git": true,
    "**/*.js": true,
    "**/*.d.ts": true,
    "**/*.spec.ts": true,
    "**/dist": true,
    "**/node_modules": true
  },
  // "files.exclude": {
  //   "**/dist": true,
  //   "**/node_modules": true,
  //   "**/tsconfig.tsbuildinfo": true
  // },
  "files.watcherExclude": {
    "**/node_modules": true,
    "**/dist": true
  },
  "monorepoWorkspace.folders.prefix.custom": [
    { "regex": "audio", "prefix": "ðŸŽ§ " },
    { "regex": "web", "prefix": "âš“ " }
  ],
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[postcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Red",
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
