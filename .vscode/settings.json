{
  "deno.enable": false,
  // "workbench.colorTheme": "SynthWave '84",
  "typescript.tsdk": "node_modules/typescript/lib",
  "windicss.enableCodeFolding": true,
  "windicss.foldByLength": true,
  "windicss.includeLanguages": {},
  "windicss.sortOnSave": true,
  "css.validate": false,
  "css.lint.emptyRules": "ignore",
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  // "workbench.colorTheme": "Ayu Mirage Bordered",
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "scss.lint.emptyRules": "ignore",
  "less.lint.emptyRules": "ignore",
  // "vite.autoStart": true
  "files.associations": {
    ".prettierrc": "toml",
    "*.mjs": "ignore",
  },
  "jest.autoRun": "off",
  // "evenBetterToml.debug": true,
  // "evenBetterToml.schema.associations": {
    // "^(.*(/|\\\\)\\.?taplo\\.toml|\\.?taplo\\.toml)$": "taplo://taplo.toml"
    // "^(.*my-settings\\.toml)$": "./schemas/my-settings.json"
  // },
  "evenBetterToml.taploConfigEnabled": true,
  "evenBetterToml.commands.clearCache": true,
  "evenBetterToml.commands.downloadSchemas": false,
  "evenBetterToml.commands.copyJsonAsToml": false,
  "evenBetterToml.commands.copyTomlAsJson": false,
  "evenBetterToml.commands.pasteJsonAsToml": false,
  "evenBetterToml.commands.pasteTomlAsJson": false,
}
